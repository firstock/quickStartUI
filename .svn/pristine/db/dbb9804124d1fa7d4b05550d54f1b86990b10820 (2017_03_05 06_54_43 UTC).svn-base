@CHARSET "UTF-8";

/*user_list.jsp ui */
#uList_wrap{
  width:500px; padding:10px;
  border:1px dotted skyblue;
  margin-top:20px; border-radius:15px;
  margin-left:auto; 
  margin-right:auto;
  box-shadow:10px 10px 50px skyblue;
  font-size:12px;	
}
#uList_t .id{
	width:100px;text-align:center
}
#uList_t .pwd{
	width:150px;text-align:center;
}
#uList_t .name{width:150px;text-align:center;}
#uList_wrap .uList_title{
	margin:20px 0 0 180px; width:160px;
	padding:3px; background:skyblue; 
	border-radius:10px;
	font-size:20px; text-align:center;
	font-family : 맑은 고딕;
}
#uList_count{
	margin:20px 0 0 320px; background:pink;
	padding:3px; border-radius:15px; text-align:center;
	font-size:14px;width:150px;
}
#uList_t{
	margin:20px 0 0 43px;
}
#uList_menu{
    margin:20px 0 0 360px;	
}

