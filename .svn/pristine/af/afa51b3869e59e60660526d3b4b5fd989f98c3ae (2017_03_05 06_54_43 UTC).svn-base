@CHARSET "UTF-8";
* {margin:0; padding:0;}
a { text-decoration:none; color:black;}
body {margin:0 auto; text-align:center;}
#top { width:100%; height:300px; text-align: center; background-repeat:no-repeat;
	background-image:url("../img/bg1.jpg"); background-size:100% auto;}
#top span {font-size:2em; color:black; font-weight:bold; line-height:9em; }
#container {width:100%; height:100%;}
table { border-collapse:collapse; width:70%; margin:0 auto;}
th {color:#e62e00; border-bottom:2px solid #ff704d; padding:10px 5px;}
td {color:#ff5c33; }
#thumb { width:80px; height:80px;}